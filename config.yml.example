# General Settings
debug: false
admin_password: "CHANGEME" # Password to become an admin and use commands
logging:
  level: INFO # DEBUG, INFO, WARNING, ERROR
  file: data/bot.log # Optional: log to file

accounts:
- accid: 1
  server: https://nine.testrun.org
  use_if_exists: true
  proxy:
    type: http # Supports 'http', 'https', 'socks5', or 'ss' (Shadowsocks)
    host: your_proxy_host
    port: your_proxy_port
    username: your_proxy_username # For Shadowsocks, this is the 'method'
    password: your_proxy_password


channels_to_mirror:
  - tgid: -1001234567890
    username: your_telegram_channel
    channel_photo_mode: auto
    delta_chat_account: 1
    delta_chat_chat_id: 10
    send_start: false
    chat_id: 10
    photo:
      enable: true
      message: "[Photo]"
    video:
      enable: true
      message: "[Video]"
  - username: your_another_telegram_channel_alternative_join_with_username_on_init
    delta_chat_account: 1
    delta_chat_chat_id: 11
    photo:
      enable: false
      message: "[این پیام شامل عکس هست]"
    video:
      enable: false
      message: "[این پیام شامل ویدیو هست]"
  


telegram:
  api_hash: your_api_hash
  api_id: your_api_id
  phone: '+1234567890'
  device_model: 'iPhone 12 Pro Max'
  system_version: 'iOS 14.4'
  app_version: '8.2.2'
  lang_code: 'en'
  system_lang_code: 'en'
# History Resend Settings
history_resend:
  enabled: true
  limit: 10
